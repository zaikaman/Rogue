# Server Configuration
PORT=4000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Alchemy RPC Provider (Polygon Amoy Testnet)
POLYGON_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your_api_key_here

# OpenAI Configuration (REQUIRED for ADK-TS agents)
# API key for OpenAI or OpenAI-compatible services
OPENAI_API_KEY=your_openai_api_key_here

# Base URL for OpenAI API
# - Default: https://api.openai.com/v1 (official OpenAI)
# - For Azure OpenAI: https://YOUR_RESOURCE.openai.azure.com
# - For local LLMs (Ollama, LM Studio, etc.): http://localhost:11434/v1
# - For other OpenAI-compatible services: use their base URL
# NOTE: Custom baseURL support has been patched into the local @iqai/adk package
OPENAI_BASE_URL=https://api.openai.com/v1

# Model to use for all agents
# Supported: gpt-3.5-*, gpt-4*, gpt-4o*, gpt-5*, o1-*, o3-*
OPENAI_MODEL=gpt-4o-mini

# Chainlink Oracle Addresses (Polygon Amoy Testnet - ChainID: 80002)
# Note: POL/USD is the main feed on Amoy (MATIC rebranded to POL)
CHAINLINK_USDC_USD=0x001382149eBa3441043c1c66972b4772963f5D43
CHAINLINK_MATIC_USD=0x001382149eBa3441043c1c66972b4772963f5D43

# Token Addresses (Polygon Amoy Testnet - ChainID: 80002)
# Note: Use testnet faucet at https://faucets.chain.link/polygon-amoy
USDC_ADDRESS=0x41E94Eb019C0762f9Bfcf9Fb1E58725BfB0e7582
WMATIC_ADDRESS=0x360ad4f9a9A8EFe9A8DCB5f461c4Cc1047E1Dcf9

# Frax API (if applicable)
FRAX_API_URL=https://api.frax.finance

# Aave Subgraph (Polygon Amoy Testnet)
AAVE_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/aave/protocol-v3-polygon-amoy

# JWT Secret for wallet signature verification
JWT_SECRET=your_random_jwt_secret_here

# Cron Configuration
AUTONOMOUS_SCAN_INTERVAL=4h
AUTONOMOUS_COMPOUND_INTERVAL=24h
