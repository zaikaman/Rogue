# Server Configuration
PORT=4000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Alchemy RPC Provider
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your_api_key_here

# OpenAI Configuration (REQUIRED for ADK-TS agents)
# API key for OpenAI or OpenAI-compatible services
OPENAI_API_KEY=your_openai_api_key_here

# Base URL for OpenAI API
# - Default: https://api.openai.com/v1 (official OpenAI)
# - For Azure OpenAI: https://YOUR_RESOURCE.openai.azure.com
# - For local LLMs (Ollama, LM Studio, etc.): http://localhost:11434/v1
# - For other OpenAI-compatible services: use their base URL
# NOTE: Custom baseURL support has been patched into the local @iqai/adk package
OPENAI_BASE_URL=https://api.openai.com/v1

# Model to use for all agents
# Supported: gpt-3.5-*, gpt-4*, gpt-4o*, gpt-5*, o1-*, o3-*
OPENAI_MODEL=gpt-4o-mini

# Chainlink Oracle Addresses (Polygon Mainnet)
CHAINLINK_USDC_USD=0xfE4A8cc5b5B2366C1B58Bea3858e81843581b2F7
CHAINLINK_MATIC_USD=0xAB594600376Ec9fD91F8e885dADF0CE036862dE0

# Token Addresses (Polygon Mainnet)
USDC_ADDRESS=0x2791Bca1f2de4661ED88A30C99A7a9449Aa84174
WMATIC_ADDRESS=0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270

# Frax API (if applicable)
FRAX_API_URL=https://api.frax.finance

# Aave Subgraph
AAVE_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/aave/protocol-v3-polygon

# JWT Secret for wallet signature verification
JWT_SECRET=your_random_jwt_secret_here

# Cron Configuration
AUTONOMOUS_SCAN_INTERVAL=4h
AUTONOMOUS_COMPOUND_INTERVAL=24h
